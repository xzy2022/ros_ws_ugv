<?xml version="1.0" encoding="UTF-8"?>
<launch>
    <arg name="robot_name" default="smart" />
    <arg name="x" default="0.0" />
    <arg name="y" default="0.0" />
    <arg name="z" default="0.0" />
    <arg name="urdf_robot_file" default="$(find my_robot_description)/urdf/smart.xacro" />

    <include file="$(find my_robot_gazebo)/launch/spawn_xacro.launch">
        <arg name="x" value="$(arg x)"/>
        <arg name="y" value="$(arg y)"/>
        <arg name="z" value="$(arg z)"/>
        <arg name="urdf_robot_file" value="$(arg urdf_robot_file)"/>
        <arg name="robot_name" value="$(arg robot_name)"/>
    </include>
</launch>
